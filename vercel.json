{
"version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "styles/style.css", "use": "@vercel/static" },
      { "src": "styles/styles.css", "use": "@vercel/static" },
    
    
    { "src": "scripts/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
