{
  "version": 2,
  "builds": [
    {
      "src": "Munchiessnack.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/*",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Munchiessnack.html"
    }
  ]
}
